package com.mit.social.bodies;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel
public class ReactTypeBody {
	private int type;

	@ApiModelProperty(required = true, allowableValues = "ADD(1), REMOVE(2)")
	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}
	
	
}
