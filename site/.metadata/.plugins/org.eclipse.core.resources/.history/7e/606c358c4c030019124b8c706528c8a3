package com.mit.react.responses;

/**
 * Created by Lap Pham on 2/24/17.
 */
public class ReactResponse {
    private int reactType;
    private int total;
    private boolean isReacted;

    public int getReactType() {
        return reactType;
    }

    public void setReactType(int reactType) {
        this.reactType = reactType;
    }

    public int getTotal() {
        return total;
    }

    public void setTotal(int total) {
        this.total = total;
    }

    public boolean isReacted() {
        return isReacted;
    }

    public void setReacted(boolean reacted) {
        isReacted = reacted;
    }
}
